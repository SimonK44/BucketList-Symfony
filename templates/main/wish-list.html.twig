{% extends 'base.html.twig' %}

{% block title %}Wish List{% endblock %}


{% block body %}

    <h2>All Wishes !</h2>
    <ul>
        {% for wish in wishes %}
            <li><a href="{{ path('app_detail', { 'id' : wish.id }) }}">{{ wish.title }}</a></li>
        {% endfor %}
    </ul>

{% endblock %}